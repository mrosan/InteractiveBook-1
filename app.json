{
	"expo": {
		"name": "Interactive Books",
		"slug": "InteractiveBooks",
		"version": "1.0.0",
		"orientation": "default",
		"icon": "./assets/interactive-logo.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/interactive-splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": [
			"**/*"
		],
		"ios": {
			"supportsTablet": true
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/interactive-logo.png",
				"backgroundColor": "#FFFFFF"
			}
		},
		"web": {
			"favicon": "./assets/favicon.png"
		}
	}
}